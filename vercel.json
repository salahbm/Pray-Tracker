{
    "buildCommand": "expo export -p web",
    "outputDirectory": "dist/client",
    "functions": {
      "api/index.ts": {
        "runtime": "@vercel/node@5.1.8",
        "includeFiles": [
          "dist/server/**",
          "node_modules/.prisma/client/**",
          "node_modules/@prisma/client/**"
        ]
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/index"
      }
    ]
  }